<h1>編集画面</h1>

<%= form_with model: @impression, url: impression_path(@impression.id), method: :patch do |f| %>
  
    <h4>勉強会名</h4>
  <%= f.text_field :impression_title %>
  
  <h4>講師名</h4>
  <%= f.text_field :name %>
  
  <h4>感想</h4>
  <%= f.text_area :impression_text %>
  
  <%= f.submit '保存' %>
  
<% end %>